  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:once": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
